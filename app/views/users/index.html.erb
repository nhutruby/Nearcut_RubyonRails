<style>
    .center {
        text-align: center;
        height: 100px;
    }
</style>
<div class="center">
  <%= form_for @user, :remote=>true, html: { multipart: true } do |f| %>
    <%= f.label :file %>
    <%= f.file_field :file %>
    <br>
    <%= f.submit "Save" %>
  <% end %>
</div>
<div id = 'results'/>

